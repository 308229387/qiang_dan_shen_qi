<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    app:contentPadding="-2dp"
    app:cardElevation="1dp"
    app:cardCornerRadius="2dip"
    android:layout_height="wrap_content"
    >

    <LinearLayout
        android:id="@+id/domesticregister_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/grap_list_selector"
        android:orientation="horizontal">

    <ImageView
        android:id="@+id/grab_style"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:paddingTop="-2dip"
        android:paddingBottom="-2dip"
        android:src="@drawable/type_back_grabbed"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dip"
        android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dip">

    <ImageView
        android:id="@+id/iv_domesticregister_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="3dip"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="11dip"
        android:src="@drawable/iv_domesticregister_label_knock" />

        <TextView
            android:id="@+id/grab_domesticregister_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/iv_domesticregister_type"
            android:layout_centerVertical="true"
            android:text="内资工商注册"
            android:textColor="@color/black"
            android:textSize="@dimen/textsize_xlarge"  />

        <TextView
            android:id="@+id/grab_domesticregister_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="18dp"
            android:text="10:28"
            android:textColor="@color/binding_list_text"
            android:textSize="@dimen/textsize_small"/>

</RelativeLayout>

    <include
        android:id="@+id/item_domesticregister"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        layout="@layout/can_fetch_list_domestic_item"
        android:layout_marginBottom="10dip"/>


    <include
        android:id="@+id/view_cash_bottom"
        layout="@layout/bottom_main_list"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginRight="16dp"/>
</LinearLayout>
    </LinearLayout>
</android.support.v7.widget.CardView>