<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="25dp"
    android:layout_marginTop="10dp"
    android:orientation="vertical"
    app:cardElevation="2dp">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:text="基本信息"
            android:textColor="@color/black"
            android:textSize="15sp"/>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="注册类型"
                android:textColor="@color/light_text_color"
                android:textSize="@dimen/textsize_large" />

            <TextView
                android:id="@+id/detail_item_common_international_text_register_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text=""
                android:textColor="@color/order_detail_text"
                android:textSize="@dimen/textsize_large" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="注册区域"
                android:textColor="@color/light_text_color"
                android:textSize="@dimen/textsize_large" />

            <TextView
                android:id="@+id/detail_item_common_international_text_register_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text=""
                android:textColor="@color/order_detail_text"
                android:textSize="@dimen/textsize_large" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="注册时间"
                android:textColor="@color/light_text_color"
                android:textSize="@dimen/textsize_large" />

            <TextView
                android:id="@+id/detail_item_common_international_text_register_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text=""
                android:textColor="@color/order_detail_text"
                android:textSize="@dimen/textsize_large" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="经营业务"
                android:textColor="@color/light_text_color"
                android:textSize="@dimen/textsize_large" />

            <TextView
                android:id="@+id/detail_item_common_international_text_business"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="未填写"
                android:textColor="@color/order_detail_text"
                android:textSize="@dimen/textsize_large" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="所属行业"
                android:textColor="@color/light_text_color"
                android:textSize="@dimen/textsize_large" />

            <TextView
                android:id="@+id/detail_item_common_international_text_register_industry"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text=""
                android:textColor="@color/order_detail_text"
                android:textSize="@dimen/textsize_large"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="代理记账"
                android:textColor="@color/light_text_color"
                android:textSize="@dimen/textsize_large" />

            <TextView
                android:id="@+id/detail_item_common_international_text_delegate_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text=""
                android:textColor="@color/order_detail_text"
                android:textSize="@dimen/textsize_large" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="代理地址"
                android:textColor="@color/light_text_color"
                android:textSize="@dimen/textsize_large" />

            <TextView
                android:id="@+id/detail_item_common_international_text_delegate_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text=""
                android:textColor="@color/order_detail_text"
                android:textSize="@dimen/textsize_large" />
        </LinearLayout>
</LinearLayout>
