<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/grap_list_selector"
    app:cardElevation="2dp"

    >
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="100dp"
    android:layout_marginLeft="10dp"
    android:id="@+id/ll"
    android:orientation="vertical">

    <!-- 必须有的一块 -->


        <RelativeLayout
            android:id="@+id/rl_individual_title_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dip">
            <TextView
                android:layout_centerVertical="true"
                android:id="@+id/tv_project"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:text="工商注册"
                android:textColor="#000000"
                android:textSize="17sp" />

            <TextView
                android:layout_centerVertical="true"
                android:id="@+id/tv_adapter_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dip"
                android:textColor="@color/timeline_color"
                android:text="16-06-20  11:30"
                android:textSize="13sp"/>

        </RelativeLayout>


          <include
              android:id="@+id/individual"
              layout="@layout/can_orderlist_domestic_personal_item"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:layout_marginTop="5dip"/>

        <LinearLayout
            android:id="@+id/ll_individual_order_state"
            android:layout_width="match_parent"
            android:layout_height="5dip"
            android:layout_marginRight="10dip"
            android:background="@color/divider"
            android:layout_marginTop="5dip">

            <ImageView
                android:id="@+id/iv_individual_order_state_line"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" />

        </LinearLayout>

        <TextView
            android:id="@+id/tv_individual_order_state"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="10dip"
            android:text="待服务"
            android:textColor="@color/order_state_default"/>



</LinearLayout>
</android.support.v7.widget.CardView>