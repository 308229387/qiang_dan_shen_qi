<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/qiangdan_bg_item"
    android:orientation="vertical">

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:background="@color/divider" />


    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp">

        <TextView
            android:id="@+id/tv_type_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="注册类型"
            android:textColor="@color/light_text_color"
            android:textSize="@dimen/textsize_large" />

        <TextView
            android:id="@+id/tv_type_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/tv_type_title"
            android:textColor="@color/order_detail_text"
            android:textSize="@dimen/textsize_large" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp">

        <TextView
            android:id="@+id/tv_location_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="所在区域"
            android:textColor="@color/light_text_color"
            android:textSize="@dimen/textsize_large" />

        <TextView
            android:id="@+id/tv_location_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/tv_location_title"
            android:textColor="@color/order_detail_text"
            android:textSize="@dimen/textsize_large" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp">

        <TextView
            android:id="@+id/tv_zx_business_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="经营业务"
            android:textColor="@color/light_text_color"
            android:textSize="@dimen/textsize_large" />

        <TextView
            android:id="@+id/tv_zx_business_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/tv_zx_business_title"
            android:textColor="@color/order_detail_text"
            android:textSize="@dimen/textsize_large" />

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp">

        <TextView
            android:id="@+id/tv_zx_time_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="所属行业"
            android:textColor="@color/light_text_color"
            android:textSize="@dimen/textsize_large" />

        <TextView
            android:id="@+id/tv_zx_time_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/tv_zx_time_title"
            android:textColor="@color/order_detail_text"
            android:textSize="@dimen/textsize_large" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp">

        <TextView
            android:id="@+id/tv_size_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="代理记账"
            android:textColor="@color/light_text_color"
            android:textSize="@dimen/textsize_large" />

        <TextView
            android:id="@+id/tv_size_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/tv_size_title"
            android:textColor="@color/order_detail_text"
            android:textSize="@dimen/textsize_large" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp">

        <TextView
            android:id="@+id/tv_budget_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="代理地址"
            android:textColor="@color/light_text_color"
            android:textSize="@dimen/textsize_large" />

        <TextView
            android:id="@+id/tv_budget_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/tv_budget_title"
            android:textColor="@color/order_detail_text"
            android:textSize="@dimen/textsize_large" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp">

        <TextView
            android:id="@+id/tv_lf_time_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="注册时间"
            android:textColor="@color/light_text_color"
            android:textSize="@dimen/textsize_large" />

        <TextView
            android:id="@+id/tv_lf_time_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/tv_lf_time_title"
            android:textColor="@color/order_detail_text"
            android:textSize="@dimen/textsize_large" />

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp">

        <TextView
            android:id="@+id/tv_needs_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="特殊需求"
            android:textColor="@color/light_text_color"
            android:textSize="@dimen/textsize_large" />

        <TextView
            android:id="@+id/tv_needs_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/tv_needs_title"
            android:textColor="@color/order_detail_text"
            android:textSize="@dimen/textsize_large" />

    </RelativeLayout>

</LinearLayout>

