<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wuba.lego"
    android:versionCode="2"
    android:versionName="2.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="14" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application>
        <service
            android:name="com.wuba.lego.service.LegoSendAndWriteService"
            android:exported="true" >
        </service>
        <service
            android:name="com.wuba.lego.service.LegoReportStrategyService"
            android:enabled="true"
            android:exported="true" >
        </service>

        <receiver android:name="com.wuba.lego.service.LegoRecevier" >
            <intent-filter>
                <action android:name="com.wuba.lego.clientlog.receiver" />
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
    </application>

</manifest>