<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/personalregister_item"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_marginBottom="5dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginTop="5dp"
    app:cardElevation="2dp"
    android:background="@drawable/grap_list_selector"
    android:layout_height="wrap_content"
   >

   <!-- <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="14dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="14dp" >-->
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/divider"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/divider"
        android:layout_alignParentBottom="true"
        />
        <TextView
            android:id="@+id/grab_personalregister_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:text="sss"
            android:textColor="#000000"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/grab_personalregister_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:text="ss"
            android:layout_alignBaseline="@id/grab_personalregister_title"
            android:textColor="#000000"
            android:textSize="12sp" />
   <!-- </RelativeLayout>-->

    <View
        android:id="@+id/view"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:layout_below="@id/grab_personalregister_title"
        android:background="@color/divider" />

    <include
        android:id="@+id/item_personalregister"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/view"
        android:layout_marginTop="10dp"
        layout="@layout/can_fetch_list_personal_domestic_item"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_below="@id/item_personalregister"
        android:background="@color/divider"
        />

    <include
        layout="@layout/bottom_main_list"
        android:layout_below="@id/item_personalregister"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        />
</RelativeLayout>
